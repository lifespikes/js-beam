{
  "compilerOptions": {
    "composite": true,
    "target": "ES6",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext",
      "es2015.symbol"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "vite/client"
    ],
    "baseUrl": ".",
  },
  "exclude": [
    "node_modules"
  ]
}
